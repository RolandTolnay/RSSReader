  target 'RSSReaderTests' do
    inherit! :search_paths
    
    pod 'FeedKit', '~> 6.0'
    pod 'Nuke', '~> 5.0'
    pod 'PullToRefreshSwift', '~> 3.0'
    pod 'SideMenu'
  end

end
